<%= simple_form_for(@user) do |f| %>
 <%= f.input :first_name, placeholder: "mojito" %>
 <%= f.input :last_name, placeholder: "mojito" %>
 <%= f.input :date_of_birth, placeholder: "mojito" %>
 <%= f.input :ss_number, placeholder: "mojito" %>
 <%= f.input :address, placeholder: "mojito" %>
 <%= f.input :password, placeholder: "mojito" %>
 <%= f.input :email, placeholder: "mojito" %>
 <%= f.input :phone_number, placeholder: "mojito" %>
 <%= f.input :avatar, placeholder: "mojito" %>
 <%= f.input :photo %>
 <%= f.input :photo_cache, as: :hidden %>
 <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
